--- 
layout: layout.njk 
--- 

<div class="grid-container margin-y-4 usa-prose">
  {{ content | safe }} 
</div>

<div class="grid-container margin-y-4">
  <div class="grid-row">
    <div class="grid-col desktop:grid-col-8">
      <h3>{{grantees.title}}</h3>
      <ul>
      {% for p in collections.grantees %}
          <li><a href="{{p.url}}">{{p.data.title}}</a></li>
      {% endfor %}
      </ul>
    </div>
    <div class="grid-col desktop:grid-col-8">
      <h3>{{auditors.title}}</h3>
      {% for p in collections.auditors %}
          <li><a href="{{p.url}}">{{p.data.title}}</a></li>
      {% endfor %}
    </div>
    <div class="grid-col desktop:grid-col-8">
      <h3>{{feds.title}}</h3>
      {% for p in collections.feds %}
          <li><a href="{{p.url}}">{{p.data.title}}</a></li>
      {% endfor %}
    </div>
  </div>
</div>

